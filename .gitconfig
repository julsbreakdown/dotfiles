[user]
    name = julien.waddle@camptocamp.com
    email = julien.waddle@camptocamp.com

[color]
    diff=auto
    status=auto
    branch=auto
    interactive=auto
    ui=auto

[alias]
    st = status
    df = diff
    co = checkout
    ci = commit
    br = branch
    inbr = log ^master
    amend = commit --amend
    st = status
    who = shortlog -sne
    oneline = log --pretty=oneline --abbrev-commit --graph
    changes = diff --name-status
    dic = diff --cached
    diffstat = diff --stat
    lol = log --decorate --pretty=oneline --abbrev-commit --all --graph
	lgb = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset%n' --abbrev-commit --date=relative --branches

[core]
	excludesfile = /home/jwaddle/.gitignore
[rebase]
	autostash = on
	autosquash = true
